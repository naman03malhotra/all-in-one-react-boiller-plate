{
  "name": "projectx_fe_custom",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn kill && yarn start:devnodeserver & yarn start:devserver",
    "start:devserver": "NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js",
    "start:devnodeserver": "NODE_ENV=development nodemon --watch ./source -d 200ms -V app.js",
    "start:prod": "yarn build:prodonlocal && yarn run:prodonlocal",
    "build:prodonlocal": "rm -rf dist && NODE_ENV=production webpack --config config/webpack.config.prod.js && NODE_ENV=production webpack --config config/webpack.config.node.js",
    "run:prodonlocal": "NODE_ENV=production node app.js",
    "kill": "kill-port 9000 && kill-port 3040"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "proxy-middleware": "^0.15.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^4.2.1",
    "file-loader": "^6.0.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^4.3.0",
    "ignore-loader": "^0.1.2",
    "kill-port": "^1.6.1",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "raw-loader": "^4.0.1",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.0"
  }
}
